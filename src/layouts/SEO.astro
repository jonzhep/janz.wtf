---
const title = 'jz';
const description = 'jan zoltkowski website';
const ogImage = 'janz.wtf/images/og-image.png';
---

<meta name="og:site_name" property="og:site_name" content="jz" />
<meta name="og:type" property="og:type" content="website" />
<meta property="og:description" content="A website for jan zoltkowski" />
<meta property="og:title" content={title} />
<meta property="og:image" content={ogImage} />
<meta name="twitter:card" content="summary_large_image" />
<meta name="twitter:site" content="abniico" />
<meta name="twitter:title" content={title} />
<meta name="twitter:description" content={description} />
<meta name="twitter:image" content={ogImage} />
<meta name="description" content={description} />
